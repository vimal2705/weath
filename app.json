{
  "expo": {
    "name": "weather",
    "slug": "weather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive_Icon.png",
    "splash": {
      "image": "./assets/Splash_Screen_of_weather.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.vimal2705.weather"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_Icon.png",
        "backgroundColor": "#2196F3"
      },
      "package": "com.vimal2705.weather"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
